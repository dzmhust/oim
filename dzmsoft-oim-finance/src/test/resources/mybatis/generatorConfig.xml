<generatorConfiguration >
	<!-- 本地数据库jdbc驱动包路径 -->
	<classPathEntry location="D:/Program Files (x86)/apache-maven-3.1.1/repository/mysql/mysql-connector-java/5.1.35/mysql-connector-java-5.1.35.jar" />
	<!-- jdbc数据库连接配置 -->
	<context id="mysqlTables" targetRuntime="MyBatis3">
		<property name="mergeable" value="false"></property>
  	
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/walle"
			userId="root"
			password="root">
		</jdbcConnection>
    
		<javaTypeResolver >
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<!-- targetPackage为本地实体类项目相对路径    targetProject为项目包绝对路径-->
	
		<javaModelGenerator targetPackage="com.dzmsoft.oim.finance.pojo" targetProject="dzmsoft-oim-finance">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="com.dzmsoft.oim.finance.mapping"  targetProject="dzmsoft-oim-finance">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER" targetPackage="com.dzmsoft.oim.finance.dao"  targetProject="dzmsoft-oim-finance">
			<property name="enableSubPackages" value="true" />
